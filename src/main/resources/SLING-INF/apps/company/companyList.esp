     <section class="company_list_page">

    <div class="container">
        <div class="row">
            <div class="col-sm-10">
            	<div class="row">
            		<div class="col-xs-12">
            			<div class="well well-sm company-header bg-primary">
            				<div class="row">
            				<form action="<%=request.getContextPath()%>/servlet/service/globalsearch.searchlist" type="GET" id="searchForm1">
            					<div class="col-sm-3">
            					
            					<select  data-placeholder="Search By Category" class="chosen-select form-control" id="idPlaceHolder" name="core1">
                                            <option value=""></option>
                                            <option value="company">Company</option>
											<option value="group">Group</option>
											<option value="person">Person</option>
                                        </select></div>
                                        <input type="hidden" name="userId" value="<%=request.getRemoteUser().replace("@","_")%>" id="<%=request.getRemoteUser().replace("@","_")%>"/>
            					<div class="col-sm-3"><input type="text" class="form-control" placeholder="Search by name" id="searchByName1" name="searchByName"></div>
            					<div class="col-sm-6"><span class="input-group-btn">
                                            <button class="btn btn-primary" type="button" onclick="validateSearch1()">Search</button>
                                        </span></div>
								</form>
            				</div>
            			</div>
            		</div>
            	</div>
                <div class="row">
                    <div class="col-xs-12 ">
						<div class="row">
							<div class="col-sm-3">
								<div class="panel panel-primary">
							      <div class="panel-heading">
							        <h3 class="panel-title" id="panel-title">Result for other categories</h3>
							      </div>
							      <div class="panel-body">
							        <p>Company Name <a href="#">1111 results</a></p>
							        <p>Trade Name <a href="#">221 results</a></p>
							        <p>Executive name 3333 results</p>
							        <p>News 4444 results</p>
							        <p>Contacts 5555 results</p>
							      </div>
							    </div>
							</div>
							
							<div class="col-sm-9">
								<div class="row">
									<div class="demo content4">
									            		<%
            		var count=request.getAttribute("total");

 var result = (count/8);
var integerPart = Math.floor(result);
if(count-(integerPart*10)!=0){
integerPart=integerPart+1;
}

			var	companyNode=request.getAttribute("companylist");
		
			
%>
<%
for(i=0;i<companyNode.size();i++){
											var cmpNode=companyNode.get(i);
			
												%>
							            <div class="col-md-6 col-xs-12 custom-width">
							            	<div class="well well-sm company-list">
							            		<div class="row">
							            		<div class="col-sm-4 col-xs-4 image">
							            			<% if(cmpNode.hasNode("BasicInfo")){
							            			var basicInfo = cmpNode.getNode("BasicInfo");
												               	%>
																	<img width="88.32" height="115.95" href='<%=request.getContextPath()%>/servlet/company/show.view?compN=<%=basicInfo.title ?basicInfo.title :""%>' src="<%=request.getContextPath()%><%=basicInfo.companyLogo?basicInfo.companyLogo:""%>" alt=""/>
													            <%}else{  %>
													                     <img src="<%=request.getContextPath()%>/apps/user/css/images/photo.gif" alt=""/> 
													            <%}%>
							            		</div>
							            		<% if(cmpNode.hasNode("BasicInfo")){%>
							            		<div class="col-sm-8 col-xs-8 text">
							            			<h3><a href='<%=request.getContextPath()%>/servlet/company/show.view?compN=<%=cmpNode.companyNumber ?cmpNode.companyNumber :""%>'><%=basicInfo.companyName?basicInfo.companyName:""%></a></h3>
							            			<p><a href="http://<%=basicInfo.website?basicInfo.website:""%>" target="_blank"><%=basicInfo.website?basicInfo.website:""%></a></p>
							            			<p><a href="#">Industries: </a><%=basicInfo.industries?basicInfo.industries:""%></p>
							            			<%
							            			if(cmpNode.hasNode("taxation")){
							            			var taxNode = cmpNode.getNode("taxation");
							            			%>
							            			<p><a href="#">Place: </a><%=taxNode.foundedPlace?taxNode.foundedPlace:""%></p>
							            			<p><a href="#">Founded: </a><%=taxNode.founded?taxNode.founded:""%></p>
							            			<!--<p><a href="#">Turn Over: </a><%=basicInfo.turnOver?basicInfo.turnOver:""%></p>-->
							            			<%}%>
							            			
							            		</div>
							            		<%}%>
							            		</div>
							            	</div>
							            </div>
<%}%>
           
									</div>
						            <div class="col-xs-12">
						            	<div class="demo demo4_bottom"></div>
						            </div>
						                 <script type="text/javascript">
						                        $('.demo4_bottom,.demo4_top').bootpag({
						                             total: <%=integerPart%>,
						                            page: 1,
						                            maxVisible: 5,
						                            
						                        }).on("page", function(event, num){
						                            //show
						                            $(".pace-active").show();
						                            $(".content4").load("http://prod.bizlem.io:8082/portal/servlet/common/search.company?from="+ num +"&to=10", function(){
						                              $(".pace-active").hide();
						                              //hide
						                            });
						                            // or some ajax content loading...
						                        }).find('.pagination');
						                        
						                    </script>
								</div>
							</div>
						</div>
                    </div>
                </div>
            </div>
            <div class="col-sm-2">
