        <script>

            function openRecomend()
            {
                $("#hideFilter").hide();
                $("#hideFilter1").hide();
                $("#hideFilter2").hide();
                $("#preFilter").show();
                $("#preFilter1").show();
                $("#preFilter2").show();
                $("#preFilter3").show();
                $("#preFilter4").show();
                $("#preFilter5").show();
                $("#preFilter6").show();
                $("#preFilter7").show();
            }

            function closeRecommend()
            {
                $("#hideFilter").show();
                $("#hideFilter1").show();
                $("#hideFilter2").show();
                $("#preFilter").hide();
                $("#preFilter1").hide();
                $("#preFilter2").hide();
                $("#preFilter3").hide();
                $("#preFilter4").hide();
                $("#preFilter5").hide();
                $("#preFilter6").hide();
                $("#preFilter7").hide();
            }

            var i = 0;
            function openFilter() {

                if (i == 0) {
                    $("#hideFilter").show();
                    $("#hideFilter1").show();
                    $("#hideFilter2").show();
                    i = 1;
                } else {
                    $("#hideFilter").hide();
                    $("#hideFilter1").hide();
                    $("#hideFilter2").hide();
                    $("#preFilter").hide();
                    $("#preFilter1").hide();
                    $("#preFilter2").hide();
                    $("#preFilter3").hide();
                    $("#preFilter4").hide();
                    $("#preFilter5").hide();
                    $("#preFilter6").hide();
                    $("#preFilter7").hide();

                    i = 0;
                }

            }

            var j = 0;
            function openOtherReq() {

                if (j == 0) {
                    $("#hideOthReq").show();
                    $("#hideOthReq1").show();
                    $("#hideOthReq2").show();
                    j = 1;
                } else {
                    $("#hideOthReq").hide();
                    $("#hideOthReq1").hide();
                    $("#hideOthReq2").hide();
                    j = 0;
                }

            }


            function fetchPrd() {
                var prdNode = document.getElementById("productId").value;
                var vUrl = '<%=request.getContextPath()%>/servlet/exhibition/flash.eventNameSuggetion'
                $.ajax({
                    url: vUrl,
                    data: {"paravalue": prdNode, "paraname": "name", "path": "/content/product/products/", "isparent": "0"
                    },
                    success: function(data) {
                        console.log(data);
                        var parsed = (JSON.parse(data)).length - 1;
                        var vData = parsed + " suppliers can give quotations."
                        $("#quoSupplr").show();
                        $("#quoSupplr").html(vData);
                        $("#tellSupplr").hide();

                    },
                    error: function(data) {
                        alert("error--" + data);
                    }
                });
            }

            function submitAdd() {
                var prdNode = document.getElementById("producturl").value;
                var vUrl = '<%=request.getContextPath()%>/servlet/common/search.fetchthroughurl'
                $.ajax({
                    url: vUrl,
                    data: {"producturl": prdNode},
                    success: function(data) {
                        console.log(data);
                        var d = data.split("888");
                        if (d != null && d[0] == "success") {
                            $("#prodtext").html(d[2]);
                            $("#prodimgae").html(d[3]);
                            $("#h4title").html(d[1]);
                            document.getElementById("hidprdname").value = d[1];
                            document.getElementById("hidprdcat").value = d[4];
                            document.getElementById("hidprdimage").value = d[3];
                            
                            document.getElementById("urlprod").style.display = '';
                            document.getElementById("urlprod1").style.display = '';
                        }else{
                         $("#h4title").html(data);
                            document.getElementById("urlprod").style.display = 'none';
                            document.getElementById("urlprod1").style.display = 'none';
                        }
                    },
                    error: function(data) {
                        alert("error--" + data);
                    }
                });
            }

        </script>
